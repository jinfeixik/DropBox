/*main内容显示*/

.right-content {
	margin-left: 237px;
	background: #fff;
	height: 100%;
	min-height: 100%;
	overflow: auto;
	position: fixed;
	top: 198px;
}

.content {
	position: relative;
	min-height: 100%;
	margin: 0px;
}

#page_content {
	position: absolute;
	width: 100%;
	height: 100%;
}

#page_content .search {
	padding: 32px 0px 32px 24px;
	background-color: rgb(239, 244, 250);
	border: 1px solid rgb(211, 220, 232);
}

#page_content .search span {
	font-size: 20px;
	color: #25272a;
}

#page_content .dir {
	padding: 20px 0px 20px 60px;
}

.dir_box {
	width: 100%;
	margin-top: 24px;
}

#page_content .dir_small {
	height: 100%;
	line-height: 30px;
	padding: 20px 0px 20px 60px;
}

.serach_box {
	width: 202px;
	margin-top: -30px;
	z-index: 1100;
	border: 1px solid rgb(239, 244, 250);
	position: relative;
	height: 30px;
}

.serach_box img {
	width: 100%;
	cursor: pointer;
}

.serach_box input {
	background: url('../images/search_input.png');
	width: 100%;
	position: absolute;
	left: 0;
	height: 100%;
	text-indent: 2em;
}

.serach_box button {}

.nomargin {
	margin: 0px;
}

.dir_small .file_size {
	padding-right: 36px;
}

.dir_small .time {
	padding-right: 24px;
}

.dir_box:hover {
	background-color: rgb(239, 244, 250);
}



#left-small:hover{

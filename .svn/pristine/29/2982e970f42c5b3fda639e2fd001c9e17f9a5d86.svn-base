/*顶部div*/

#box {
	display: none;
	width: 200px;
	height: 150px;
	background: white;
	border: 1px solid #333;
	padding: 12px;
	text-align: center;
	border-radius: 20px;
	margin-left: -30px;
}

img.touxiang {
	border-radius: 50%;
	width: 35px;
	height: 35px;
}

img.touxiang1 {
	border-radius: 50%;
	width: 50px;
	height: 50px;
}

input[class=seafch-input] {
	width: 240px;
	box-sizing: border-box;
	border: 1px solid #ccc;
	border-radius: 4px;
	font-size: 13px;
	background-color: eef0f6;
	background-image: url('searchicon.png');
	background-position: 10px 10px;
	background-repeat: no-repeat;
	padding: 12px 20px 12px 4px;
	-webkit-transition: width 0.4s ease-in-out;
	transition: width 0.4s ease-in-out;
	height: 35px;
	margin-top: 10px;
	margin-left: -30px;
}

input[class=seafch-input]:focus {
	width: 300%;
}

.nav {
	width: 100%;
	height: 74px;
    background-color:#eef0f6;
	border-bottom: 1px solid #d3dce8;
	min-width: 1258px;

<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%
	String path = request.getContextPath();
	String basePath = request.getScheme() + "://"
			+ request.getServerName() + ":" + request.getServerPort()
			+ path + "/";
%>

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<base href="<%=basePath%>">

<title>注册</title>

<meta http-equiv="pragma" content="no-cache">
<meta http-equiv="cache-control" content="no-cache">
<meta http-equiv="expires" content="0">
<meta http-equiv="keywords" content="keyword1,keyword2,keyword3">
<meta http-equiv="description" content="This is my page">
<link rel="stylesheet" href="css/bootstrap.min.css" />
<script type="text/javascript" src="js/bootstrap.min.js"></script>
<script type="text/javascript" src="js/jquery.min.js"></script>
<style>
.leader {
	width: 1500px;
	height: 60px;
	background: #f2f2f2 no-repeat 0px;
}

.title {
	margin-left: 70px;
}

.content {
	margin-bottom: 10px;
	padding: 15px;
	line-height: 22px;
	border-left: 5px solid #009688;
	border-radius: 0 2px 2px 0;
	background-color: #f2f2f2
}

.light {
	margin-bottom: 20px;
	padding: 0;
	border: 1px solid #CED4DA;
	margin-top: 60px;
	margin-left: 400px;
	margin-right: 570px;
	border-radius: 10%;
	background: #f2f2f2;
}

.light legend {
	margin-left: 230px;
	padding: 20px 10px;
	font-size: 20px;
	font-weight: 300;
}

.form-group {
	margin-left: 70px;
	margin-top: 40px;
}
</style>


</head>

<body>

	<div class="leader">
		<div class="title">
			<font size="6" color="#FF33FF" style="font-family: STHupo;">9</font>
			<font size="6" color="#CC33FF" style="font-family: STHupo;">0</font>
			<font size="6" color="#9999FF" style="font-family: STHupo;">网</font>
			<font size="6" color="#6699FF" style="font-family: STHupo;">盘</font>
			<font size="4" color="#FF6666" style="font-family: STLiti;">不一样的</font>
			<font size="5" color="#FF3366" style="font-family: STLiti;">云</font>
			<font size="4" color="#FF0066" style="font-family: STLiti;">存储</font>
			<span style="margin-left: 650px;">我已注册 ，现在就<font size="4"><a
					href="userNamePasswordLogin.jsp">登录</a>
			</font>
			</span>
		</div>
		<div class="light">
			<fieldset>
				<legend >
					<font size="5" color="#007bff" >注册</font>
				</legend>
				<div class="container">
					<div class="row clearfix">
						<div class="col-md-12 column">
							<form action="registerTest.form" class="form-horizontal" role="form" style="margin-top: 30px;">
								<div class="form-group" style="margin-top: -20px">
									<div class="col-sm-10">
										<input type="text" class="form-control"
										name="userName"	id="inputPassword3" placeholder="请输入6-14位中英文字符的用户名" />
									</div>
								</div>

								<div class="form-group">
									<div class="col-sm-10">
										<input type="password" class="form-control"
										name="userPassword"	id="inputPassword3" placeholder="请输入8-14位密码" />
									</div>
								</div>

								<div class="form-group">
									<div class="col-sm-10">
										<input type="email" class="form-control" id="inputEmail3"
										name="userEmail" placeholder="请输入邮箱" />
									</div>
								</div>

								<div class="form-group" style="margin-left: 140px;">
									<div class="col-sm-offset-2 col-sm-10">
										<div class="checkbox">
											<label><input type="checkbox" /><font size="3"
												color="007bff" style="font-family: simhei;">阅读并同意《XX协议》</font>
											</label>
										</div>
									</div>
								</div>
								<div class="form-group">
									<div class="col-sm-offset-2 col-sm-10">
										<input type="submit" class="btn btn-primary"
											style="width: 330px;" value="立即注册"/>
									</div>
								</div>
							</form>
						</div>
					</div>
				</div>
			</fieldset>
		</div>
	</div>

	<!--    <form action="registerTest.form" method="post">
    	<input type="text" size="20" name="userName">
    	<input type="password" size="20" name="userPassword" >
    	<input type="text" size="20" name="userEmail">
    	<input type="submit" value="注册" >
    </form> -->

</body>
</html>
